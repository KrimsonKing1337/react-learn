{
  "name": "@vtb-ib/chat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "build:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development --env.buildTarget=fs",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "npm-run-all --parallel lint:*",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint-fix": "npm-run-all format lint-fix:*",
    "lint-fix:ts": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "lint-fix:css": "stylelint \"src/**/*.scss\" --fix",
    "format": "prettier --write \"src/**/*.{json,{j,t}s{,x},scss,}\"",
    "test": "jest",
    "watch-tests": "jest --watch",
    "coverage": "jest --coverage",
    "changelog:major": "changelog -M && git add CHANGELOG.md && npm version major",
    "changelog:minor": "changelog -m && git add CHANGELOG.md && npm version minor",
    "changelog:patch": "changelog -p && git add CHANGELOG.md && npm version patch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:lint"
    }
  },
